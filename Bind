//Example 1
this.car="Honda Civic w/Ugly Spoiler";
var marksGarage ={
car:"Aston Martin",
getCar: function() {
return this.car;
}};
console.log(markGarage.getCar());



Output:"Aston Martin"
//Example 2

this.car="Honda Civic w/Ugly Spoiler";
var marksGarage ={
car:"Aston Martin",
getCar: function() {
return this.car;
}};
console.log(markGarage.getCar());

Output:
